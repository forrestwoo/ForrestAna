<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.forrest.mapper.ListResponseMapper">

	<insert id="insertListResponse" parameterType = "listResponse">
	insert into listresponse (id,userid,content)   
    value(#{id},#{userID},#{content})  
	</insert>
</mapper>
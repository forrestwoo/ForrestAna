<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.forrest.mapper.TempTableMapper">
	<insert id="insertTempData" parameterType="tempTable"
		useGeneratedKeys="true">
		<selectKey resultType="int" keyProperty="id" order="AFTER">
			SELECT
			LAST_INSERT_ID()
		</selectKey>
		insert into temp_data
		(name)
		value (#{name})
	</insert>
	<select id="selectUseridFromTemp" resultType="string">
	select userid from temp_data
	</select>
	
</mapper>
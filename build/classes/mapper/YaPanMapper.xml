<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.forrest.mapper.YaPanMapper">
	<insert id="insertYaPan" parameterType="yaPan">
		insert into yapan(mid,a1,a2,a3,aa1,aa2,aa3,b1,b2,b3,bb1,bb2,bb3)
		value (#{mid},#{a1},#{a2},#{a3},#{aa1},#{aa2},#{aa3},#{b1},#{b2},#{b3},#{bb1},#{bb2},#{bb3})
	</insert>
</mapper>